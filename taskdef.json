{ 
   "executionRoleArn":"arn:aws:iam::000000000000:role/Task_Definition_Role",
   "containerDefinitions":[ 
      { 
         "name":"my-webapp",
         "image":"my-webapp-repo",
         "essential":true,
         "portMappings":[ 
            { 
               "hostPort":80,
               "protocol":"tcp",
               "containerPort":80
            }
         ]
      }
   ],
   "requiresCompatibilities":[ 
      "EC2",
      "FARGATE"
   ],
   "networkMode":"awsvpc",
   "cpu":"256",
   "memory":"512",
   "family":"my-webapp"
}
